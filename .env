VITE_API_URL=https://backendbalompie-production.up.railway.app/

# URL del frontend
VITE_FRONTEND_URL=https://balompie-shop-production.up.railway.app/


# WhatsApp Business API
VITE_WHATSAPP_PHONE=+1234567890
VITE_WHATSAPP_MESSAGE=Hola! Me interesa un producto de Balompie Shop

# Información de la tienda
VITE_STORE_NAME=Balompie Shop
